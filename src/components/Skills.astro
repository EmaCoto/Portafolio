const doubleSkills = [...skills, ...skills];
---

<section id="skills" class="overflow-hidden pt-10 z-50">
  <TitleSecundary h2="Conocimientos" span="Stack Tecnológico"/>

  <div class="relative flex overflow-x-hidden group/marquee mt-10">
    <div class="py-12 animate-marquee flex whitespace-nowrap gap-12 items-center main-marquee" style="will-change: transform; backface-visibility: hidden;">
      {doubleSkills.map((skill) => (
        <div class="skill-card relative flex flex-col items-center justify-center min-w-30 md:min-w-40 transition-all duration-500">
          
          <div 
            class="skill-icon w-12 h-12 md:w-16 md:h-16 transition-all duration-500 flex items-center justify-center [&>svg]:w-full [&>svg]:h-full [&>svg]:block" 
            set:html={skill.svg}
          />
          
          <span class="skill-name absolute -bottom-4 text-[#72bf78] font-mono text-[10px] uppercase tracking-[0.3em] transition-all duration-300 whitespace-nowrap opacity-100 translate-y-2.5 md:opacity-0 md:translate-y-0">
            {skill.name}
          </span>
        </div>
      ))}
    </div>

    <div class="pointer-events-none absolute inset-y-0 left-0 w-32 bg-linear-to-r from-[#0a0709] to-transparent z-10"></div>
    <div class="pointer-events-none absolute inset-y-0 right-0 w-32 bg-linear-to-l from-[#0a0709] to-transparent z-10"></div>
  </div>
</section>

<style>
  @reference "../style/global.css";

  @keyframes marquee {
    0% { transform: translate3d(0, 0, 0); }
    100% { transform: translate3d(-50%, 0, 0); }
  }

  .animate-marquee {
    animation: marquee 12s linear infinite;
  }


  /* --- MÓVIL (Colores originales vivos) --- */
  .skill-icon {
    filter: saturate(1) drop-shadow(0 0 8px rgba(114, 191, 120, 0.2));
    opacity: 1;
  }

  /* --- ESCRITORIO (md) --- */
  @media (min-width: 768px) {
    .skill-icon {
      filter: grayscale(1) opacity(0.4);
    }

    /* Al entrar al carrusel, todos se opacan más excepto el que tocas */
    .main-marquee:hover .skill-icon {
      filter: grayscale(1) opacity(0.2) blur(1px);
    }

    /* El seleccionado recupera color y brillo */
    .skill-card:hover .skill-icon {
      filter: grayscale(0) opacity(1) drop-shadow(0 0 15px rgba(114, 191, 120, 0.4));
      transform: scale(1.2);
    }

    .skill-card:hover .skill-name {
      opacity: 1;
      transform: translateY(10px);
    }

    .animate-marquee {
      animation: marquee 25s linear infinite;
    }
  }
</style>